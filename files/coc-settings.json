{
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "coc.preferences.snippetStatusText": " ",
  "suggest.languageSourcePriority": 99,
  "suggest.timeout": 500,
  "suggest.snippetIndicator": " \ue796",
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf6a6",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "diagnostic.displayByAle": true,
  // coc-snippets
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  // coc-explorer
  "explorer.width": 40,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.sources": [
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.icon.customIcons": {
    "icons": {
      "folderClosed": {
        "code": " ",
        "color": "#00afaf"
      },
      "folderOpened": {
        "code": " ",
        "color": "#00afaf"
      }
    }
  },
  "explorer.file.child.template": "[selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][readonly] [linkIcon & 1][link growRight 1 omitCenter 5][size]",
  "explorer.keyMappings.global": {
    "<tab>": "toggleSelection",
    "<cr>": ["expanded?", "collapse", "expand"],
    "gl": "expandRecursive",
    "gh": "collapseRecursive",
    "e": "open",
    "s": "open:split",
    "E": "open:vsplit",
    "t": "open:tab",
    "Y": "copyFilepath",
    "y": "copyFilename",
    "c": "copyFile",
    "x": "cutFile",
    "p": "pasteFile",
    "d": "delete",
    "D": "deleteForever",
    "a": "addFile",
    "A": "addDirectory",
    "r": "rename",
    ".": "toggleHidden",
    "R": "refresh",
    "f": "search",
    "F": "searchRecursive",
    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer"
  },
  "explorer.icon": {
    "expanded": " ",
    "collapsed": " ",
    "selected": ""
  },
  "explorer.git.icon": {
    "unmodified": "",
    "mixed": "* ",
    "modified": " ",
    "untracked": " ",
    "deleted": " ",
    "added": " "
  },
  // coc-emmet
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact"
  },
  // coc-html
  "html.filetypes": ["html", "handlebars", "blade"],
  // coc-python
  "python.jediEnabled": true,
  "python.pythonPath": "/usr/bin/python3",
  "python.linting": {
    "enabled": false,
    "pylintEnabled": false,
    "flake8Enabled": true,
    "flake8Args": ["--ignore=E501,E402,F401,E701"]
  },
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Args": ["--ignore=E501"],
  // coc-translator
  "translator": {
    "toLang": "ru",
    "engines": ["google"]
  },
  // coc-go
  "go.goplsOptions": {
    "usePlaceholders": true,
    "completeUnimported": true,
    "deepCompletion": true
  },
  // coc-lua
  "lua.version": "5.3",
  "lua.commandPath": "/home/horseinthesky/.luarocks/bin/lua-lsp",
  // coc-yaml
  "yaml.validate": false,
  "yaml.format.enable": true,
  "yaml.completion": true,
  "prettier.formatterPriority": -1,
  "prettier.printWidth": 0,
  "yaml.schemas": {
    "kubernetes": "/*.yaml"
  }
}
